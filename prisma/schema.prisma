generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id            Int      @id @default(autoincrement())
  bride         String?
  groom         String?
  brideEmail    String?  @unique
  groomEmail    String?  @unique
  bridePassword String?
  groomPassword String?
  createdAt     DateTime @default(dbgenerated("'2022-08-02 16:09:43.655206'::timestamp without time zone")) @db.Timestamp(6)
}
